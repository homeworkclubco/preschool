{
  "name": "preschool",
  "version": "0.1.0",
  "description": "A minimal, extensible CSS framework and web component library",
  "type": "module",
  "main": "./dist/preschool.umd.cjs",
  "module": "./dist/preschool.js",
  "exports": {
    ".": {
      "import": "./dist/preschool.js",
      "require": "./dist/preschool.umd.cjs"
    },
    "./styles": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "watch": "vite build --watch",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "docs:dev": "cd docs && npm run dev",
    "docs:build": "cd docs && npm run build",
    "docs:preview": "cd docs && npm run preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "css",
    "framework",
    "web-components",
    "design-system",
    "ui",
    "accessible"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "lit": "^3.2.1",
    "postcss-import-ext-glob": "^2.1.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@storybook/addon-a11y": "^9.1.10",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/web-components-vite": "^9.1.10",
    "@vitest/browser": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "axe-core": "^4.10.2",
    "playwright": "^1.49.1",
    "postcss": "^8.4.49",
    "postcss-custom-media": "^11.0.4",
    "postcss-import": "^16.1.1",
    "postcss-nesting": "^13.0.1",
    "storybook": "^9.1.10",
    "vite": "^7.1.9",
    "vitest": "^3.2.4"
  }
}
